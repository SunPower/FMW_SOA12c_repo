            portType="ns1:SFDCLogin_BPEL" operation="process"
            bpelx:invokeAsDetail="no"/>
    <assign name="AsgnSessionHeader">
      <copy>
        <from variable="InvSFDCLogin_OV" part="payload"
              query="/ns1:SFDCLoginResp/ns1:sessionId"/>
        <to variable="header" query="/ns2:SessionHeader/ns2:sessionId"/>
      </copy>
    </assign>
    <assign name="AsgnPartnerReference">
      <copy>
        <from variable="InvSFDCLogin_OV" part="payload"
              query="/ns1:SFDCLoginResp/ns1:serverUrl"/>
        <to variable="partnerReference"
            query="/ns3:EndpointReference/ns3:Address"/>
      </copy>
      <copy>
        <from variable="partnerReference"/>
        <to partnerLink="SFDC"/>
      </copy>
    </assign>
    <assign name="TransOracleAccountNumbers">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('Transformations/TransOracleAccountNumbers.xsl', $inputVariable.payload)"/>
        <to variable="ORACLE_ACCOUNT_NUMBERS"/>
      </copy>
    </assign>
    <assign name="AsgnInvSFDC_Query">
      <copy>
        <from expression="concat(&quot;Select Id, Oracle_Account_Number__c  From Account where Oracle_Account_Number__c in (&quot;, substring(bpws:getVariableData('ORACLE_ACCOUNT_NUMBERS','/ns6:OracleAccountNumbers/ns6:OracleAccountNumber'),1,string-length(bpws:getVariableData('ORACLE_ACCOUNT_NUMBERS','/ns6:OracleAccountNumbers/ns6:OracleAccountNumber'))-1),&quot;)&quot;, &quot;and IsPartner = true and Lease_Order_Placed__c != true&quot;)"/>
        <to variable="InvSFDC_Query_IV" part="parameters"
            query="/ns2:query/ns2:queryString"/>
      </copy>
    </assign>
    <invoke name="InvSFDC_Query" inputVariable="InvSFDC_Query_IV"
            outputVariable="InvSFDC_Query_OV" partnerLink="SFDC"
            portType="ns2:Soap" operation="query" bpelx:invokeAsDetail="no"
            bpelx:inputHeaderVariable="header"/>
            
