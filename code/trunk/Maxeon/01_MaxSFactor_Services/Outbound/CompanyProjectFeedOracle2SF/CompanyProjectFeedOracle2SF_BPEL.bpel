<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  Oracle JDeveloper BPEL Designer 
  
  Created: Tue Sep 22 13:51:49 IST 2015
  Author:  sn38854
  Type: BPEL 1.1 Process
  Purpose: One Way BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="CompanyProjectFeedOracle2SF_BPEL"
               targetNamespace="http://xmlns.oracle.com/SuccessFactor/CompanyProjectFeedOracle2SF/CompanyProjectFeedOracle2SF_BPEL"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/SuccessFactor/CompanyProjectFeedOracle2SF/CompanyProjectFeedOracle2SF_BPEL"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/SuccessFactor/CompanyProjectFeedOracle2SF/SelectPickListFeedFromOracle"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/SelectPickListFeedFromOracle"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/file/SuccessFactor/CompanyProjectFeedOracle2SF/WriteCompanyProjectFeedToLocal"
         xmlns:ns4="http://xmlns.oracle.com/SOAUtilityServices/FileToFTPMoveUtilitySerivce/FileToFTPMoveUtilitySerivce_BPEL"
         xmlns:ns5="http://www.sunpowercorp.com/SOAUtilityServices/FileToFTPMoveUtilityService/xsd/v1"
         xmlns:ns6="http://xmlns.oracle.com/testingapplication/CommonSFDCErrorLogService/CallCommSFDCErrMediator"
         xmlns:ns7="http://www.sunpowercorp.com/SOAUtilityServices/CommonSFDCErrorLogService/CommonSFDCErrorLogService_BPEL"
         xmlns:ns8="http://xmlns.oracle.com/SOAUtilityServices/FileUtilityService/FileUtilityService_BPEL"
         xmlns:ns9="http://www.sunpowercorp.com/SunPowerDevelopment/FileUtilityService/v1/xsd">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <bpelx:annotation>
    <bpelx:analysis>
      <bpelx:property name="wsdlFile">
        <![CDATA[CompanyProjectFeedOracle2SF_BPEL.wsdl]]>
      </bpelx:property>
    </bpelx:analysis>
  </bpelx:annotation>
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="companyprojectfeedoracle2sf_bpel_client" partnerLinkType="client:CompanyProjectFeedOracle2SF_BPEL" myRole="CompanyProjectFeedOracle2SF_BPELProvider"/>
    <partnerLink name="SelectPickListFeedFromOracle"
                 partnerLinkType="ns1:SelectPickListFeedFromOracle_plt"
                 partnerRole="SelectPickListFeedFromOracle_role"/>
    <partnerLink name="WriteCompanyProjectFeedToLocal"
                 partnerLinkType="ns3:Write_plt" partnerRole="Write_role"/>
    <partnerLink name="FileToFTPMoveUtilitySerivce"
                 partnerLinkType="ns4:FileToFTPMoveUtilitySerivce_BPEL"
                 partnerRole="FileToFTPMoveUtilitySerivce_BPELProvider"/>
    <partnerLink name="CommonSFDCErrorLogService"
                 partnerLinkType="ns6:CommonSFDCErrorLogService"
                 partnerRole="execute_ptt"/>
    <partnerLink name="FileUtilityService"
                 partnerLinkType="ns8:FileUtilityService_BPEL"
                 partnerRole="FileUtilityService_BPELProvider"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:CompanyProjectFeedOracle2SF_BPELRequestMessage"/>
    <variable name="InvPickListFeedFromEBS_IV"
              messageType="ns1:SelectPickListFeedFromOracleInput_msg"/>
    <variable name="InvPickListFeedFromEBS_OV"
              messageType="ns1:SelectPickListFeedFromOracleOutputCollection_msg"/>
    <variable name="Title" type="xsd:string"/>
    <variable name="DateTime" type="xsd:dateTime"/>
    <variable name="BatchSize" type="xsd:int"/>
    <variable name="MinRows" type="xsd:int"/>
    <variable name="MaxRows" type="xsd:int"/>
    <variable name="FileName" type="xsd:string"/>
    <variable name="allRecordsCompleted" type="xsd:string"/>
    <variable name="InvWriteCompanyProjectFeedToLocal_IV"
              messageType="ns3:Write_msg"/>
    <variable name="InvWriteCompanyProjectFeedToLocal_OV"
              messageType="ns3:Output_msg"/>
    <variable name="RecordCount" type="xsd:int"/>
    <variable name="InvFileToFTPMoveUtilitySerivce_IV"
              messageType="ns4:FileToFTPMoveUtilitySerivce_BPELRequestMessage"/>
    <variable name="InvFileToFTPMoveUtilitySerivce_OV"
              messageType="ns4:FileToFTPMoveUtilitySerivce_BPELResponseMessage"/>
    <variable name="InvCommonSFDCErrorLogService_IV"
              messageType="ns6:requestMessage"/>
    <variable name="InvFileUtilityService_Move_IV"
              messageType="ns8:FileUtilityService_BPELRequestMessage"/>
    <variable name="InvFileUtilityService_Move_OV"
              messageType="ns8:FileUtilityService_BPELResponseMessage"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence3">
        <assign name="AsgnCommonSFDCErrorLogService">
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:InstanceNumber"/>
          </copy>
          <copy>
            <from expression='ora:getPreference("InterfaceNum")'/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:InterfaceNumber"/>
          </copy>
          <copy>
            <from expression='ora:getPreference("InterfaceName")'/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:InterfaceName"/>
          </copy>
          <copy>
            <from expression='ora:getPreference("ProcessName")'/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:ProcessName"/>
          </copy>
          <copy>
            <from expression='ora:getPreference("FaultCode")'/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:Fault/ns7:FaultCode"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:Fault/ns7:FaultDescription"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:Fault/ns7:FaultMessage"/>
          </copy>
          <copy>
            <from expression="xp20:current-dateTime()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:Fault/ns7:FaultTime"/>
          </copy>
          <copy>
            <from variable="FileName"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:UniqueIdentNum/ns7:UniqueIdentifier"/>
          </copy>
          <copy>
            <from expression='ora:getPreference("SendEmail")'/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:SendEmail"/>
          </copy>
          <copy>
            <from expression='ora:getPreference("FaultSystemName")'/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:FaultSystemName"/>
          </copy>
          <copy>
            <from expression="'E'"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request"
                query="/ns7:CommonSFDCErrorLog/ns7:CommonErrorHandlerInputDetails/ns7:status"/>
          </copy>
        </assign>
        <invoke name="InvCommonSFDCErrorLogService"
                inputVariable="InvCommonSFDCErrorLogService_IV"
                partnerLink="CommonSFDCErrorLogService"
                portType="ns6:execute_ptt" operation="execute"
                bpelx:invokeAsDetail="no"/>
        <terminate name="Terminate"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in CompanyProjectFeedOracle2SF_BPEL.wsdl) -->
    <receive name="receiveInput" partnerLink="companyprojectfeedoracle2sf_bpel_client" portType="client:CompanyProjectFeedOracle2SF_BPEL" operation="process" variable="inputVariable" createInstance="yes"/>
    <assign name="SetTitle">
      <copy>
        <from expression="oraext:setFlowInstanceTitle('CompanyProjectFeedOracle2SF')"/>
        <to variable="Title"/>
      </copy>
    </assign>
    <assign name="intialiseAllVariables">
      <copy>
        <from expression='xp20:format-dateTime(xp20:current-dateTime(),"[M01][D01][Y0001][H01][m01][s01]")'/>
        <to variable="DateTime"/>
      </copy>
      <copy>
        <from expression='ora:getPreference("sizeOfBunch")'/>
        <to variable="BatchSize"/>
      </copy>
      <copy>
        <from expression="1"/>
        <to variable="MinRows"/>
      </copy>
      <copy>
        <from expression='ora:getPreference("sizeOfBunch")'/>
        <to variable="MaxRows"/>
      </copy>
      <copy>
        <from expression="concat('ExpenseAccount_picklist_values_',$DateTime,'.csv')"/>
        <to variable="FileName"/>
      </copy>
      <copy>
        <from expression='"false"'/>
        <to variable="allRecordsCompleted"/>
      </copy>
    </assign>
    <while name="WhileAllRowsCompleted" condition="$allRecordsCompleted='false'">
      <sequence>
        <assign name="AssignEBSInput">
          <copy>
            <from variable="MinRows"/>
            <to variable="InvPickListFeedFromEBS_IV"
                part="SelectPickListFeedFromOracleInput_msg"
                query="/ns2:SelectPickListFeedFromOracleInput/ns2:minrow"/>
          </copy>
          <copy>
            <from variable="MaxRows"/>
            <to variable="InvPickListFeedFromEBS_IV"
                part="SelectPickListFeedFromOracleInput_msg"
                query="/ns2:SelectPickListFeedFromOracleInput/ns2:maxrow"/>
          </copy>
        </assign>
        <invoke name="InvCompanyProjectFeedFromEBS"
                inputVariable="InvPickListFeedFromEBS_IV"
                outputVariable="InvPickListFeedFromEBS_OV"
                partnerLink="SelectPickListFeedFromOracle"
                portType="ns1:SelectPickListFeedFromOracle_ptt"
                operation="SelectPickListFeedFromOracle"
                bpelx:invokeAsDetail="no"/>
        <assign name="asgnReceivedRecordCount">
          <copy>
            <from expression="ora:countNodes('InvPickListFeedFromEBS_OV','SelectPickListFeedFromOracleOutputCollection','/ns2:SelectPickListFeedFromOracleOutputCollection/ns2:SelectPickListFeedFromOracleOutput')"/>
            <to variable="RecordCount"/>
          </copy>
        </assign>
        <switch name="Switch1">
          <case condition="$RecordCount > 0">
            <bpelx:annotation>
              <bpelx:general>
                <bpelx:property name="userLabel">RecordsAvailable</bpelx:property>
              </bpelx:general>
            </bpelx:annotation>
            <sequence>
              <assign name="TransCompanyProjectFeedToLocal">
                <bpelx:annotation>
                  <bpelx:pattern>transformation</bpelx:pattern>
                </bpelx:annotation>
                <copy>
                  <from expression="ora:doXSLTransformForDoc('Transformations/TransCompanyProjectFeedToLocal.xsl', $InvPickListFeedFromEBS_OV.SelectPickListFeedFromOracleOutputCollection)"/>
                  <to variable="InvWriteCompanyProjectFeedToLocal_IV"
                      part="body"/>
                </copy>
              </assign>
              <invoke name="InvWriteCompanyProjectFeedToLocal"
                      inputVariable="InvWriteCompanyProjectFeedToLocal_IV"
                      outputVariable="InvWriteCompanyProjectFeedToLocal_OV"
                      partnerLink="WriteCompanyProjectFeedToLocal"
                      portType="ns3:Write_ptt" operation="Write"
                      bpelx:invokeAsDetail="no">
                <bpelx:inputProperty name="jca.file.FileName"
                                     variable="FileName"/>
              </invoke>
              <assign name="updateMinRows">
                <copy>
                  <from expression="$MinRows+ora:getPreference('sizeOfBunch')"/>
                  <to variable="MinRows"/>
                </copy>
                <copy>
                  <from expression="$MaxRows+ora:getPreference('sizeOfBunch')"/>
                  <to variable="MaxRows"/>
                </copy>
              </assign>
            </sequence>
          </case>
          <otherwise>
            <assign name="asgnRecordsCompleted">
              <copy>
                <from expression="true()"/>
                <to variable="allRecordsCompleted"/>
              </copy>
            </assign>
          </otherwise>
        </switch>
      </sequence>
    </while>
    <assign name="asgnInvFileToFTPMoveUtilitySerivce">
      <copy>
        <from variable="FileName"/>
        <to variable="InvFileToFTPMoveUtilitySerivce_IV" part="payload"
            query="/ns5:input/ns5:SourceFileName"/>
      </copy>
      <copy>
        <from expression='"COPY"'/>
        <to variable="InvFileToFTPMoveUtilitySerivce_IV" part="payload"
            query="/ns5:input/ns5:Action"/>
      </copy>
      <copy>
        <from expression='ora:getPreference("LocalFileDir")'/>
        <to variable="InvFileToFTPMoveUtilitySerivce_IV" part="payload"
            query="/ns5:input/ns5:SourceDirectory"/>
      </copy>
      <copy>
        <from expression='ora:getPreference("FileToken")'/>
        <to variable="InvFileToFTPMoveUtilitySerivce_IV" part="payload"
            query="/ns5:input/ns5:FileToken"/>
      </copy>
      <copy>
        <from variable="FileName"/>
        <to variable="InvFileToFTPMoveUtilitySerivce_IV" part="payload"
            query="/ns5:input/ns5:TargetFileName"/>
      </copy>
    </assign>
    <invoke name="InvFileToFTPMoveUtilitySerivce"
            inputVariable="InvFileToFTPMoveUtilitySerivce_IV"
            outputVariable="InvFileToFTPMoveUtilitySerivce_OV"
            partnerLink="FileToFTPMoveUtilitySerivce"
            portType="ns4:FileToFTPMoveUtilitySerivce_BPEL"
            operation="process" bpelx:invokeAsDetail="no"/>
    <assign name="AssignFileParms">
      <copy>
        <from variable="FileName"/>
        <to variable="InvFileUtilityService_Move_IV" part="payload"
            query="/ns9:FileUtilityReq/ns9:SourceFileName"/>
      </copy>
      <copy>
        <from expression='ora:getPreference("LocalFileDir")'/>
        <to variable="InvFileUtilityService_Move_IV" part="payload"
            query="/ns9:FileUtilityReq/ns9:SourcePhysicalDirectory"/>
      </copy>
      <copy>
        <from variable="FileName"/>
        <to variable="InvFileUtilityService_Move_IV" part="payload"
            query="/ns9:FileUtilityReq/ns9:TargetFileName"/>
      </copy>
      <copy>
        <from expression='ora:getPreference("ArchiveDir")'/>
        <to variable="InvFileUtilityService_Move_IV" part="payload"
            query="/ns9:FileUtilityReq/ns9:TargetPhysicalDirectory"/>
      </copy>
    </assign>
    <invoke name="InvFileUtilityService"
            inputVariable="InvFileUtilityService_Move_IV"
            outputVariable="InvFileUtilityService_Move_OV"
            partnerLink="FileUtilityService"
            portType="ns8:FileUtilityService_BPEL" operation="Move"
            bpelx:invokeAsDetail="no"/>
  </sequence>
</process>