                            <bpelx:pattern>transformation</bpelx:pattern>
                          </bpelx:annotation>
                          <copy>
                            <from expression="ora:doXSLTransformForDoc('Transformations/TrnsInvProcessOrgAssigmentsToEBSInput.xsl', $defaultParams, 'initParams', $initParams)"/>
                            <to variable="InvProcessOrgAssigmentsToEBS_IV"
                                part="XxspwrArenaOrgAssignmentsCollection"/>
                          </copy>
                        </assign>
                        <invoke name="InvProcessOrgAssigmentsToEBS"
                                inputVariable="InvProcessOrgAssigmentsToEBS_IV"
                                partnerLink="processOrgAssigmentsToEBS"
                                portType="ns4:processOrgAssigmentsToEBS_ptt"
                                operation="insert"
                                bpelx:skipCondition="ora:countNodes('initParams','/ns13:params/ns13:param') &lt; 1"
                                bpelx:invokeAsDetail="no"/>
                        <switch name="SwitchApprovedManufacturerListItems">
                          <case condition="ora:countNodes('Item','ItemDetails','/ns6:Item/ns7:ApprovedManufacturerList/ns7:ApprovedManufacturerListItem') >0">
                            <bpelx:annotation>
                              <bpelx:general>
                                <bpelx:property name="userLabel">ApprovedManufacturerListItems Available</bpelx:property>
                              </bpelx:general>
                            </bpelx:annotation>
                            <sequence name="ApprovedManufacturerListItems">
                              <assign name="TransListOfVendorsArenaASLS">
                                <bpelx:annotation>
                                  <bpelx:pattern>transformation</bpelx:pattern>
                                </bpelx:annotation>
                                <copy>
                                  <from expression="ora:doXSLTransformForDoc('Transformations/TransListOfVendors_ArenaASLS.xsl', $Item.ItemDetails, 'inputVariable.payload', $inputVariable.payload)"/>
                                  <to variable="ListOfVendorsArenaASLS"/>
                                </copy>
                              </assign>
                              <assign name="TransProcessArenaAslsToEBSInput">
                                <bpelx:annotation>
                                  <bpelx:pattern>transformation</bpelx:pattern>
                                </bpelx:annotation>
                                <copy>
                                  <from expression="ora:doXSLTransformForDoc('Transformations/TrnsProcessArenaAslsToEBSInput.xsl', $ListOfVendorsArenaASLS, 'defaultParams', $defaultParams)"/>
                                  <to variable="InvProcessArenaAslsToEBS_IV"
                                      part="XxspwrArenaAslsCollection"/>
                                </copy>
                              </assign>
                              <invoke name="InvProcessArenaAslsToEBS"
                                      inputVariable="InvProcessArenaAslsToEBS_IV"
                                      partnerLink="processArenaAslsToEBS"
                                      portType="ns16:processArenaAslsToEBS_ptt"
                                      operation="insert"
                                      bpelx:invokeAsDetail="no"/>
                            </sequence>
                          </case>
                        </switch>
                        <switch name="SwitchArenaSolutionsCustomAttributes">
                          <case condition="ora:countNodes('Item','ItemDetails','/ns6:Item/ns7:AdditionalAttributes[@groupLabel=&quot;Arena Solutions Custom Attributes&quot;]') >0">
                            <bpelx:annotation>
                              <bpelx:general>
                                <bpelx:property name="userLabel">ArenaSolutionsCustomAttributes Available</bpelx:property>
                              </bpelx:general>
                            </bpelx:annotation>
                            <sequence name="ArenaSolutionsCustomAttributes">
                              <switch name="switchSetMakeBuyCode">
                                <case condition="ora:getNodes('ListOfVendorsArenaASLS','/ns14:ListOfVendorsArenaAsls/ns14:ListOfVendorArenaAsls/ns14:ARENA_ASLS/ns14:Vendar_Name')=ora:getNodes('defaultParams','/ns12:defaultParams/ns12:supplierName')">
                                  <bpelx:annotation>
                                    <bpelx:general>
                                      <bpelx:property name="userLabel">SetMakeBuyCode=1</bpelx:property>
                                    </bpelx:general>
                                  </bpelx:annotation>
                                  <assign name="asgnMakeBuyCode">
                                    <copy>
                                      <from expression="1"
                                            bpelx:language="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"/>
                                      <to variable="defaultParams"
                                          query="/ns12:defaultParams/ns12:makeBuyCode"/>
                                    </copy>
                                  </assign>
                                </case>
                                <otherwise>
                                  <assign name="asgnMakeBuyCode">
                                    <copy>
                                      <from expression="2"/>
                                      <to variable="defaultParams"
                                          query="/ns12:defaultParams/ns12:makeBuyCode"/>
                                    </copy>
                                  </assign>
                                </otherwise>
                              </switch>
                              <assign name="TransAdditionalAttributesToCategories">
                                <bpelx:annotation>
                                  <bpelx:pattern>transformation</bpelx:pattern>
                                </bpelx:annotation>
                                <copy>
                                  <from expression="ora:doXSLTransformForDoc('Transformations/TrnsAdditionalAttributesToCategories2.xsl', $Item.ItemDetails, 'defaultParams', $defaultParams)"/>
                                  <to variable="Categories"/>
                                </copy>
                              </assign>
                              <assign name="TransProcessArenaCaregoriesToEBSInput">
                                <bpelx:annotation>
                                  <bpelx:pattern>transformation</bpelx:pattern>
                                </bpelx:annotation>
                                <copy>
                                  <from expression="ora:doXSLTransformForDoc('Transformations/TrnsProcessArenaCaregoriesToEBSInput.xsl', $Categories, 'defaultParams', $defaultParams)"/>
                                  <to variable="InvProcessArenaCategoriesToEBS_IV"
                                      part="XxspwrArenaCategoriesCollection"/>
                                </copy>
                              </assign>
                              <invoke name="InvProcessArenaCategoriesToEBS"
                                      inputVariable="InvProcessArenaCategoriesToEBS_IV"
                                      partnerLink="processArenaCategoriesToEBS"
                                      portType="ns17:processArenaCategoriesToEBS_ptt"
                                      operation="insert"
                                      bpelx:skipCondition="ora:countNodes('InvProcessArenaCategoriesToEBS_IV','XxspwrArenaCategoriesCollection','/ns19:XxspwrArenaCategoriesCollection/ns19:XxspwrArenaCategories') &lt;=0"
                                      bpelx:invokeAsDetail="no"/>
                            </sequence>
                          </case>
                        </switch>
                        <switch name="SwitchBillOfMaterialItems">
                          <case condition="ora:countNodes('Item','ItemDetails','/ns6:Item/ns7:BillOfMaterial/ns7:BillOfMaterialItem') >0">
                            <bpelx:annotation>
                              <bpelx:general>
                                <bpelx:property name="userLabel">BillOfMaterialItems Available</bpelx:property>
                              </bpelx:general>
                            </bpelx:annotation>
                            <sequence name="BillOfMaterialItems">
                              <assign name="TransInvProcessItemBOMsToEBSInput">
                                <bpelx:annotation>
                                  <bpelx:pattern>transformation</bpelx:pattern>
                                </bpelx:annotation>
                                <copy>
                                  <from expression="ora:doXSLTransformForDoc('Transformations/TrnsInvProcessItemBOMsToEBSInput.xsl', $Item.ItemDetails, 'defaultParams', $defaultParams, 'inputVariable.payload', $inputVariable.payload)"/>
                                  <to variable="InvProcessItemBOMsToEBS_IV"
                                      part="XxspwrArenaComponentsCollection"/>
                                </copy>
                              </assign>
                              <invoke name="InvProcessItemBOMsToEBS"
                                      inputVariable="InvProcessItemBOMsToEBS_IV"
                                      partnerLink="processItemBOMsToEBS"
                                      portType="ns20:processItemBOMsToEBS_ptt"
                                      operation="insert"
                                      bpelx:invokeAsDetail="no"/>
                            </sequence>
                          </case>
                        </switch>
                        <assign name="TransProcessItemtoEBSInput">
                          <bpelx:annotation>
                            <bpelx:pattern>transformation</bpelx:pattern>
                          </bpelx:annotation>
                          <copy>
                            <from expression="ora:doXSLTransformForDoc('Transformations/TrnsUpdateItemToEBSInput.xsl', $ListOfVendorsArenaASLS, 'defaultParams', $defaultParams)"/>
                            <to
                                part="XxspwrArenaItemsCollection"
                                variable="InvUpdateItemtoEBS_IV"/>
                          </copy>
                        </assign>
                        <switch name="Switch2">
                          <case condition="bpws:getVariableData('Item','ItemDetails','/ns6:Item/ns7:AdditionalAttributes/ns7:AdditionalAttribute[@ns8:name=&quot;Category Name&quot;]/@ns8:value')=&quot;Phantom&quot;">
                            <bpelx:annotation>
                              <bpelx:general>
                                <bpelx:property name="userLabel">
                                  <![CDATA[CheckPhantom]]>
                                </bpelx:property>
                              </bpelx:general>
                            </bpelx:annotation>
                            <assign name="Assign1">
                              <copy>
                                <from expression="1"/>
                                <to bpelx:language="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"
                                    variable="InvUpdateItemtoEBS_IV" part="XxspwrArenaItemsCollection"
                                    query="/ns33:XxspwrArenaItemsCollection/ns33:XxspwrArenaItems/ns33:planningMakeBuyCode"/>
                              </copy>
                            </assign>
                          </case>
                        </switch>
                        <invoke name="InvUpdateItemtoEBS"
                                inputVariable="InvUpdateItemtoEBS_IV"
                                partnerLink="updateItemStatusToEBS"
                                portType="ns32:updateItemStatusToEBS_ptt"
                                operation="update" bpelx:invokeAsDetail="no"/>
                      </sequence>
                    </case>
                  </switch>
                </sequence>
              </case>
              <otherwise>
                <sequence name="ItemNumberIsEmpty">
                  <assign name="AsignFaultVarible">
                    <copy>
                      <from expression="concat('Item with UniqueIdentifier :',string(ora:getNodes('inputVariable','payload','/ns6:ProcessArenaOracleDataFeed/ns6:DataArea/ns7:ArenaOracleDataFeed/ns7:ArenaOracleDataFeedDetails/ns7:Items/ns7:Item[$noOfItems]/@ns8:itemUniqueIdentifier')),' Received with empty Item Identifer, Hence failed to process the record for file',bpws:getVariableData('fileName'))"/>
                      <to variable="FaultVar" part="detail"/>
                    </copy>
                  </assign>
                  <throw name="ThrowInvalidItem" faultVariable="FaultVar"
                         faultName="client:InvalidItem"/>
                </sequence>
              </otherwise>
            </switch>
          </sequence>
        </scope>
      </sequence>
    </while>
    
    <assign name="AsgnInvFileMoveSvcInput">
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns6:ProcessArenaOracleDataFeed/ns5:ApplicationArea/ns5:FileInfo/ns5:sourceFileName"/>
        <to variable="InvFileMoveservice_IV" part="payload"
            query="/ns31:FileUtilityReq/ns31:SourceFileName"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns6:ProcessArenaOracleDataFeed/ns5:ApplicationArea/ns5:FileInfo/ns5:TargetFileName"/>
        <to variable="InvFileMoveservice_IV" part="payload"
            query="/ns31:FileUtilityReq/ns31:TargetFileName"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns6:ProcessArenaOracleDataFeed/ns5:ApplicationArea/ns5:FileInfo/ns5:InboundDir"/>
        <to variable="InvFileMoveservice_IV" part="payload"
            query="/ns31:FileUtilityReq/ns31:SourcePhysicalDirectory"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns6:ProcessArenaOracleDataFeed/ns5:ApplicationArea/ns5:FileInfo/ns5:SuccessDir"/>
        <to variable="InvFileMoveservice_IV" part="payload"
            query="/ns31:FileUtilityReq/ns31:TargetPhysicalDirectory"/>
      </copy>
    </assign>
    <invoke name="InvFileMoveservice" partnerLink="FileUtilityservice"
            portType="ns30:FileUtilityService_BPEL" operation="Move"
            inputVariable="InvFileMoveservice_IV"
            outputVariable="InvFileMoveservice_OV" bpelx:invokeAsDetail="no"/>
            
