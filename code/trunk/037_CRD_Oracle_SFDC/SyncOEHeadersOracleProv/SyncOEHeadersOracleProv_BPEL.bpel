    </variables>
    <faultHandlers>
        <catchAll>
            <sequence name="SeqCatchAll">
                <assign name="AsgnCommonSFDCErrorLogService">
                    <copy>
                        <from expression="ora:getCompositeInstanceId()"/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:InstanceNumber"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("InterfaceNumber")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:InterfaceNumber"/>
                    </copy>
                    <copy>
                        <from expression='"SyncOEOrdersSFDCToOracle"'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:InterfaceName"/>
                    </copy>
                    <copy>
                        <from expression='"SyncOEHeadersOracleProv"'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:ProcessName"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("Severity")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Severity"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("FaultCode")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultCode"/>
                    </copy>
                    <copy>
                        <from expression="ora:getFaultName()"/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultDescription"/>
                    </copy>
                    <copy>
                        <from expression="ora:getFaultAsString()"/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultMessage"/>
                    </copy>
                    <copy>
                        <from expression="xp20:current-dateTime()"/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultTime"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("FaultSystemName")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:FaultSystemName"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("bOpenServiceTicket")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:bOpenServiceTicket"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("CorrectiveAction")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:CorrectiveAction"/>
                    </copy>
                    <copy>
                        <from expression='ora:getPreference("SendEmail")'/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:SendEmail"/>
                    </copy>
                    <copy>
                        <from expression="'E'"/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:status"/>
                    </copy>
                    <copy>
                        <from expression="xp20:current-dateTime()"/>
                        <to variable="InvCommonSFDCErrorLogService_IV"
                            part="request"
                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:createddatetime"/>
                    </copy>
                </assign>
                <invoke name="InvCommonSFDCErrorLogService"
                        inputVariable="InvCommonSFDCErrorLogService_IV"
                        partnerLink="CommonSFDCErrorLogService"
                        portType="ns2:execute_ptt" operation="execute"
                        bpelx:invokeAsDetail="no"/>
                <terminate name="Terminate"/>
            </sequence>
        </catchAll>
    </faultHandlers>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in SyncOEHeadersOracleProv_BPEL.wsdl) -->
        <receive name="receiveInput"
                 partnerLink="syncoeheadersoracleprov_bpel_client"
                 portType="client:SyncOEHeadersOracleProv_BPEL"
                 operation="process" variable="inputVariable"
                 createInstance="yes"/>
        <assign name="SetTile">
            <copy>
                <from expression='oraext:setFlowInstanceTitle("SyncOEHeadersOracleProv")'/>
                <to variable="title"/>
            </copy>
        </assign>
        <assign name="intialiseDefaultValues">
            <copy>
                <from expression="1"/>
                <to variable="i"/>
            </copy>
            <copy>
                <from expression="false()"/>
                <to variable="faultOccured"/>
            </copy>
            <copy>
                <from expression="ora:countNodes('inputVariable','payload','/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails')"/>
                <to variable="noOfRecords"/>
            </copy>
        </assign>
        <while name="WhileForAllRecords" condition="$i &lt;= $noOfRecords">
            <sequence name="SeqProcessOrdersToOracle">
                <scope name="ScopeProcessOrdersToOracle"
                       variableAccessSerializable="no">
                    <faultHandlers>
                        <catch faultName="bpelx:remoteFault"
                               faultVariable="FaultVar">
                            <sequence name="SeqRemoteFault">
                                <assign name="AsgnCommonSFDCErrorLogService">
                                    <copy>
                                        <from expression="ora:getCompositeInstanceId()"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:InstanceNumber"/>
                                    </copy>
                                    <copy>
                                        <from expression="ora:getCompositeInstanceId()"/>
                                        <to variable="inputVariable"
                                            part="payload"
                                            query="/ns6:ProcessSyncOEOrders/ns7:ApplicationArea/ns7:BODID"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("BusinessInterfaceNumber")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:InterfaceNumber"/>
                                    </copy>
                                    <copy>
                                        <from expression='"SyncOEOrdersSFDCToOracle"'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:InterfaceName"/>
                                    </copy>
                                    <copy>
                                        <from expression='"SyncOEHeadersOracleProv"'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:ProcessName"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("Severity")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Severity"/>
                                    </copy>
                                    <copy>
                                        <from expression="xp20:current-dateTime()"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultTime"/>
                                    </copy>
                                    <copy>
                                        <from variable="FaultVar"
                                              part="summary"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultDescription"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("FaultCode")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultCode"/>
                                    </copy>
                                    <copy>
                                        <from variable="FaultVar"
                                              part="detail"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:Fault/ns3:FaultMessage"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("FaultSystemName")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:FaultSystemName"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("bOpenServiceTicket")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:bOpenServiceTicket"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("CorrectiveAction")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:CorrectiveAction"/>
                                    </copy>
                                    <copy>
                                        <from expression='ora:getPreference("SendEmailInLoop")'/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:SendEmail"/>
                                    </copy>
                                    <copy>
                                        <from expression="'E'"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:status"/>
                                    </copy>
                                    <copy>
                                        <from expression="xp20:current-dateTime()"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:createddatetime"/>
                                    </copy>
                                    <copy>
                                        <from expression="concat(ora:getNodes('inputVariable','payload','/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:ORDER_NUMBER'),'#',ora:getNodes('inputVariable','payload','/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:LastModifiedDate'))"/>
                                        <to variable="InvCommonSFDCErrorLogService_IV"
                                            part="request"
                                            query="/ns3:CommonSFDCErrorLog/ns3:CommonErrorHandlerInputDetails/ns3:UniqueIdentNum/ns3:UniqueIdentifier"/>
                                    </copy>
                                </assign>
                                <invoke name="InvCommonSFDCErrorLogService"
                                        inputVariable="InvCommonSFDCErrorLogService_IV"
                                        partnerLink="CommonSFDCErrorLogService"
                                        portType="ns2:execute_ptt"
                                        operation="execute"
                                        bpelx:invokeAsDetail="no"/>
                                <assign name="asgnInputParams">
                                    <copy>
                                        <from expression="ora:getCompositeInstanceId()"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:Instance_ID"/>
                                    </copy>
                                    <copy>
                                        <from variable="inputVariable"
                                              part="payload"
                                              query="/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:PSR_EMAIL"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:To"/>
                                    </copy>
                                    <copy>
                                        <from variable="FaultVar"
                                              part="detail"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:Fault_Message"/>
                                    </copy>
                                    <copy>
                                        <from variable="inputVariable"
                                              part="payload"
                                              query="/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:PARTNER_NAME"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:PARTNER_NAME"/>
                                    </copy>
                                    <copy>
                                        <from variable="inputVariable"
                                              part="payload"
                                              query="/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:PSR_NAME"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:PSR_NAME"/>
                                    </copy>
                                    <copy>
                                        <from variable="inputVariable"
                                              part="payload"
                                              query="/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:LastModifiedDate"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:LastModifiedDate"/>
                                    </copy>
                                    <copy>
                                        <from expression='"SyncOEHeadersOracleProv"'/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:Process_Name"/>
                                    </copy>
                                    <copy>
                                        <from variable="inputVariable"
                                              part="payload"
                                              query="/ns6:ProcessSyncOEOrders/ns6:DataArea/ns8:SyncOEOrders/ns8:SyncOEOrderHeaderDetails[$i]/ns9:ORDER_NUMBER"/>
                                        <to variable="inputParams"
                                            query="/ns14:inputParams/ns14:params/ns14:ORDER_NUMBER"/>
                                    </copy>
                                </assign>
                                <assign name="xfromEmailContent">
                                    <bpelx:annotation>
                                        <bpelx:pattern>transformation</bpelx:pattern>
                                    </bpelx:annotation>
                                    <copy>
                                        <from expression="ora:doXSLTransformForDoc('Transformations/xfromEmailContent.xsl', $inputParams)"/>
                                        <to variable="EmailContent"/>
                                    </copy>
                                </assign>
                                <scope name="Email1">
                                    <bpelx:annotation>
                                        <bpelx:pattern patternName="bpelx:email"></bpelx:pattern>
                                        <bpelx:general>
                                            <bpelx:property name="userLabel">sendEmail</bpelx:property>
                                        </bpelx:general>
                                    </bpelx:annotation>
                                    <variables>
                                        <variable name="varNotificationReq"
                                                  messageType="ns13:EmailNotificationRequest"/>
                                        <variable name="varNotificationResponse"
                                                  messageType="ns13:ArrayOfResponse"/>
                                        <variable name="NotificationServiceFaultVariable"
                                                  messageType="ns13:NotificationServiceErrorMessage"/>
                                    </variables>
                                    <sequence name="SeqEmail">
                                        <assign name="EmailParamsAssign">
                                            <copy>
                                                <from expression="string('Default')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:FromAccountName"/>
                                            </copy>
                                            <copy>
                                                <from expression="string('')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:Bcc"/>
                                            </copy>
                                            <copy>
                                                <from expression='dvm:lookupValue("oramds:/deployed-composites/default/CommonSFDCErrorLogService_rev1.0/EmailDistributionList.dvm","InterfaceNumber",ora:getPreference("BusinessInterfaceNumber"),"emailDistrubtionList","")'/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:Cc"/>
                                            </copy>
                                            <copy>
                                                <from expression="string('')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:ReplyToAddress"/>
                                            </copy>
                                            <copy>
                                                <from expression="bpws:getVariableData('EmailContent','/ns12:EmailContent/ns12:Subject')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:Subject"/>
                                            </copy>
                                            <copy>
                                                <from expression="bpws:getVariableData('EmailContent','/ns12:EmailContent/ns12:To')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:To"/>
                                            </copy>
                                            <copy>
                                                <from expression="bpws:getVariableData('EmailContent','/ns12:EmailContent/ns12:body')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:Content/ns13:ContentBody"/>
                                            </copy>
                                            <copy>
                                                <from expression="string('text/html; charset=UTF-8')"/>
                                                <to variable="varNotificationReq"
                                                    part="EmailPayload"
                                                    query="/EmailPayload/ns13:Content/ns13:MimeType"/>
                                            </copy>
                                        </assign>
                                        <invoke name="InvokeNotificationService"
                                                portType="ns13:NotificationService"
                                                partnerLink="NotificationService1"
                                                inputVariable="varNotificationReq"
                                                outputVariable="varNotificationResponse"
                                                operation="sendEmailNotification"/>
                                    </sequence>
                                </scope>
                                 <assign name="asgnInvInsertCRD_Audit_LOG">
                                    <copy>
                                        <from expression="ora:getCompositeInstanceId()"/>
                                        <to variable="InvInsertCRD_Audit_LOG_Input"
                                            part="CrdAuditLogCollection"
                                            query="/ns16:CrdAuditLogCollection/ns16:CrdAuditLog/ns16:instanceId"/>
                                    </copy>
                                    <copy>
                                        <from variable="inputParams"
                                              query="/ns14:inputParams/ns14:params/ns14:Fault_Message"/>
                                        <to variable="InvInsertCRD_Audit_LOG_Input"
                                            part="CrdAuditLogCollection"
                                            query="/ns16:CrdAuditLogCollection/ns16:CrdAuditLog/ns16:message"/>
                                    </copy>
