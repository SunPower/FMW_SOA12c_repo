<xsd:schema targetNamespace="http://www.sunpowercorp.com/Noun/ACBClaimToSFDC/V1" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sunpowercorp.com/Noun/ACBClaimToSFDC/V1" xmlns:Os="http://www.sunpowercorp.com/Common/ACBClaimToSFDCCommonTypes/V1">
