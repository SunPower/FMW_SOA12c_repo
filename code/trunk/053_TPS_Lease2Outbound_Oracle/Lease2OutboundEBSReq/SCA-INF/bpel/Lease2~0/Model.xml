          <copy>
            <from expression="concat(&quot;SoNumber -&quot;,bpws:getVariableData('rcvInputVariable','XxspwrLease2OutboundDetailsCollection','/ns5:XxspwrLease2OutboundDetailsCollection/ns5:XxspwrLease2OutboundDetails/ns5:soNumber'),&quot;LeaseNumber-&quot;,bpws:getVariableData('rcvInputVariable','XxspwrLease2OutboundDetailsCollection','/ns5:XxspwrLease2OutboundDetailsCollection/ns5:XxspwrLease2OutboundDetails/ns5:leaseNumber'))"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:UniqueIdentNum/ns18:UniqueIdentifier"/>
          </copy>
          <copy>
            <from expression="ora:getPreference(&quot;SendEmail&quot;)"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:SendEmail"/>
          </copy>
          <copy>
            <from expression="'E'"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:status"/>
          </copy>
          <copy>
            <from expression="xp20:current-dateTime()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:createddatetime"/>
          </copy>
        </assign>
        <invoke name="InvCommonSFDCErrorLogService" inputVariable="InvCommonSFDCErrorLogService_IV" partnerLink="CommonSFDCErrorLogService" portType="ns17:execute_ptt" operation="execute" bpelx:invokeAsDetail="no" id="BpInv1"/>
        <terminate name="Terminate" id="BpTer0"/>
