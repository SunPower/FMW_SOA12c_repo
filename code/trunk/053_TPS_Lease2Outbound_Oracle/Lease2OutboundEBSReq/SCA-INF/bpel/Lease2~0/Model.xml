    <assign name="SetTitle" id="BpAss2">
      <copy>
        <from expression="&quot;Lease2OutboundEBSReq&quot;"/>
        <to variable="title"/>
      </copy>
      <copy>
        <from expression="oraext:setFlowInstanceTitle($title)"/>
        <to variable="title"/>
      </copy>
    </assign>
    <assign name="TransEBSInputToLease2OutboundSvc" id="BpAss3">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('Transformations/TransEBSInputToLease2OutboundSvc.xsl', $rcvInputVariable.XxspwrLease2OutboundDetailsCollection)"/>
        <to variable="InvLease2OutboundSvc_IV" part="parameters"/>
      </copy>
    </assign>
    <invoke name="InvLease2OutboundSvc" inputVariable="InvLease2OutboundSvc_IV" outputVariable="InvLease2OutboundSvc_OV" partnerLink="Lease2OutboundSvc" portType="ns2:Lease2OutboundCanonical" operation="Lease2Outbound" bpelx:invokeAsDetail="no" id="BpInv2"/>
  </sequence>
