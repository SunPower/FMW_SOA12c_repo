        <assign name="AssgnCommonSFDCErrorLogService" id="BpAss1">
          <copy>
            <from expression="ora:getCompositeInstanceId()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:InstanceNumber"/>
          </copy>
          <copy>
            <from expression="ora:getPreference(&quot;InterfaceNumber&quot;)"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:InterfaceNumber"/>
          </copy>
          <copy>
            <from expression="&quot;Lease2Outbound&quot;"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:InterfaceName"/>
          </copy>
          <copy>
            <from expression="&quot;Lease2OutboundEBSReq&quot;"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:ProcessName"/>
          </copy>
          <copy>
            <from expression="ora:getPreference(&quot;Severity&quot;)"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:Severity"/>
          </copy>
          <copy>
            <from expression="ora:getPreference('FaultCode')"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:Fault/ns18:FaultCode"/>
          </copy>
          <copy>
            <from expression="ora:getFaultAsString()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:Fault/ns18:FaultDescription"/>
          </copy>
          <copy>
            <from expression="ora:getFaultName()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:Fault/ns18:FaultMessage"/>
          </copy>
          <copy>
            <from expression="xp20:current-dateTime()"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:Fault/ns18:FaultTime"/>
          </copy>
          <copy>
            <from expression="ora:getPreference(&quot;FaultSystemName&quot;)"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:FaultSystemName"/>
          </copy>
          <copy>
            <from expression="ora:getPreference(&quot;bOpenServiceTicket&quot;)"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:bOpenServiceTicket"/>
          </copy>
          <copy>
            <from expression="ora:getPreference(&quot;CorrectiveAction&quot;)"/>
            <to variable="InvCommonSFDCErrorLogService_IV" part="request" query="/ns18:CommonSFDCErrorLog/ns18:CommonErrorHandlerInputDetails/ns18:CorrectiveAction"/>
